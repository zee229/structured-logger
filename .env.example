# Environment configuration for structured-logger
# Copy this file to .env and fill in your actual values

# Sentry Configuration
SENTRY_DSN=https://your-sentry-dsn@sentry.io/project-id
SENTRY_ENVIRONMENT=development
SENTRY_RELEASE=1.1.0

# Logging Configuration
LOG_LEVEL=INFO
ENABLE_JSON_LOGGING=true

# Test Configuration
TEST_USER_ID=test_user_123
TEST_COMPANY_ID=test_company_456
TEST_REQUEST_ID=test_request_789

# Railway Configuration (for production)
RAILWAY_ENVIRONMENT=production
RAILWAY_SERVICE_NAME=structured-logger

# Optional: Database or other service URLs for testing
# DATABASE_URL=postgresql://user:pass@localhost:5432/testdb
# REDIS_URL=redis://localhost:6379/0
